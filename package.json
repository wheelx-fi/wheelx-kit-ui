{
  "name": "@wheelx/widget",
  "version": "0.0.4",
  "description": "Reusable WheelX bridge and swap widget for Next.js applications.",
  "license": "MIT",
  "author": "WheelX",
  "homepage": "https://github.com/wheelx-fi/wheelx-kit-ui",
  "bugs": {
    "url": "https://github.com/wheelx-fi/wheelx-kit-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheelx-fi/wheelx-kit-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "wheelx",
    "widget",
    "bridge",
    "swap",
    "web3",
    "nextjs",
    "react"
  ],
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "build": "pnpm run clean && tsc -p tsconfig.build.json && mkdir -p dist/internal && cp -R src/internal/assets dist/internal/assets",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "predemo:dev": "pnpm build",
    "demo:dev": "pnpm --dir demo dev",
    "predemo:build": "pnpm build",
    "demo:build": "pnpm --dir demo build"
  },
  "sideEffects": false,
  "dependencies": {
    "@binance/w3w-rainbow-connector-v2": "^1.0.6",
    "@chakra-ui/react": "^3.17.0",
    "@emotion/react": "^11.14.0",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.12",
    "@wagmi/core": "^2.19.0",
    "axios": "^1.12.0",
    "bi-fraction": "^1.2.1",
    "ethers": "^6.14.1",
    "next-themes": "^0.4.6",
    "react-icons": "^5.5.0",
    "viem": "^2.33.3",
    "wagmi": "^2.16.3",
    "zustand": "^5.0.9"
  },
  "peerDependencies": {
    "next": ">=15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "typescript": "^5.9.2"
  }
}
